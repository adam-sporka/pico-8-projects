version 41
__lua__
x=64
y=64
p=0

cx=24
cy=24

function pickup()
	cx=rnd()*128
	cy=rnd()*128
	sfx(0)
end

function _update()
	if btn(0) then x-=1 end
	if btn(1) then x+=1 end
	if btn(2) then y-=1 end
	if btn(3) then y+=1 end
	if x<0 then x=0 end
	if x>120 then x=120 end
	if y<0 then y=0 end
	if y>120 then y=120 end
	p+=0.1
	p=p%6
	
	if x>=cx-7 and x<=cx+7 and y>=cy-7 and y<=cy+7 then
		pickup()
	end
	
	dx=cx-x
	dy=cy-y
	
	cx+=rnd()*1-0.5+dx*0.003 
	cy+=rnd()*1-0.5+dy*0.003
	
	if cx<0 then cx=0 end
	if cx>120 then cx=120 end
	if cy<0 then cy=0 end
	if cy>120 then cy=120 end
	
end

function _draw()
	cls()
	spr(p+16,x,y)
	spr(1,cx,cy)
end
