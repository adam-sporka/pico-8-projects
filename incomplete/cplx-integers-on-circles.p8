pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
cls()

function draw_stuff(mode)
	a=0 b=64
	for y=1,64 do
		for x=1,64 do
			c_test=sqrt(x*x+y*y)
			if c_test==flr(c_test) and c_test<=64 then
				if mode==0 then
					circ(0,0,c_test,1)
				else
					pset(x,y,7)
					pset(c_test,0,pget(c_test,0)+1)
					pset(0,c_test,pget(0,c_test)+1)
					if x<=y then
						print(x..":"..y.." "..c_test,a,b,5)
						a+=48
						if a>=128 then
							a=0
							b+=6
						end
					end
				end
			end
		end
	end
end

draw_stuff(0)
draw_stuff(1)

function _update()
end
