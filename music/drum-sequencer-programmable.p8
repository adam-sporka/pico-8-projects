pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- 2-bar rhythm micro-machine
-- by @adam_sporka
function _init()
	cartdata("aj_rhythm")
	disp={}
	playing=0
	curs=0
	for a=0,31 do
		disp[a]=peek(0x5e00+a)
		set_note_by_disp(a)
	end
end

function set_note(ptn,row,nte,tbr,vol,fx)
	b0=nte%64+(shl(tbr%4,6))
	b1=shr(tbr,2)+shl(vol,1)+shl(fx,4)
	a=0x3200+ptn*0x44+row*2
	poke(a,b0)
	poke(a+1,b1)
end

function get_note(ptn,row)
	a=0x3200+ptn*0x44+row*2
	b=peek(a)+shl(peek(a+1),8)
	nte=b%64
	tbr=shr(b,6)%8
	vol=shr(b,3)%8
	fx=shr(b,3)%8
	return nte,tbr,vol,fx
end

function offset_disp(row,step)
	disp[row]+=step
	disp[row]=disp[row]%9
	poke(0x5e00+row,disp[row])
end

function set_note_by_disp(row)
	if disp[row]==0 then
		set_note(0,row,0,0,0,0)
	elseif disp[row]==1 then
		set_note(0,row,0xc,0,7,3)
	elseif disp[row]==2 then
		set_note(0,row,0x20,6,4,5)
	elseif disp[row]==3 then
		set_note(0,row,0x3c,4,1,5)
	elseif disp[row]==4 then
		set_note(0,row,0x3a,4,2,5)
	elseif disp[row]==5 then
		set_note(0,row,0x20,0,2,3)
	elseif disp[row]==6 then
		set_note(0,row,0x18,0,2,3)
	elseif disp[row]==7 then
		set_note(0,row,0x10,0,3,3)
	elseif disp[row]==8 then
		set_note(0,row,0x23,4,2,6)
	end
end

function _update()
	row=stat(20)
	if (btnp(5)) then
		if (playing==1) then 
			music(-1)
			curs=0
		else 
			music(0)
			end
		playing=1-playing
	end
	if (playing==1) then
	else
		if (btnp(0)) curs-=1
		if (btnp(1)) curs+=1
	 curs=curs%32
		if (btnp(2)) offset_disp(curs,1)
		if (btnp(3)) offset_disp(curs,-1)
		set_note_by_disp(curs)
	end
end

function draw_machine()
	row=stat(20)
	for a=0,31 do
		nte,tbr,vol,fx=get_note(0,a)
		c=5
		if (a%4==0) c=6
		if (row==a) c+=8
		x=a*4
		y=48
		line(x,y+6,x+2,y+6,c)
		if playing==1 then
			if (row==a) then
				line(x,y+5,x+2,y+5,c)
			end
		else
			if (curs==a) then
				line(x,y+5,x+2,y+5,c)
			end
		end
		if disp[a]==1 then
			print("b",x,y,12)
		elseif disp[a]==2 then
			print("s",x,y,11)
		elseif disp[a]==3 then
			print(".",x,y,14)
		elseif disp[a]==4 then
			print(":",x,y,15)
		elseif disp[a]==5 then
			print("@",x,y,10)
		elseif disp[a]==6 then
			print("@",x,y,9)
		elseif disp[a]==7 then
			print("@",x,y,8)
		elseif disp[a]==8 then
			print("%",x,y,7)
		end
	end
end

function _draw()
	cls(0)
	draw_machine()
end

