pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- 2-bar rhythm micro-machine
-- by @adam_sporka
function _init()
	cartdata("aj_rhythm")
	disp={}
	playing=0
	curs=0
	for a=0,31 do
		disp[a]=peek(0x5e00+a)
		set_note_by_disp(a)
	end
end

function set_note(ptn,row,nte,tbr,vol,fx,play)
	b0=nte%64+(shl(tbr%4,6))
	b1=shr(tbr,2)+shl(vol,1)+shl(fx,4)
	a=0x3200+ptn*0x44+row*2
	poke(a,b0)
	poke(a+1,b1)
	if play then
		a=0x3200+31*0x44
		poke(a,b0)
		poke(a+1,b1)
		sfx(31)
	end
end

function get_note(ptn,row)
	a=0x3200+ptn*0x44+row*2
	b=peek(a)+shl(peek(a+1),8)
	nte=b%64
	tbr=shr(b,6)%8
	vol=shr(b,3)%8
	fx=shr(b,3)%8
	return nte,tbr,vol,fx
end

function offset_disp(row,step)
	disp[row]+=step
	disp[row]=disp[row]%9
	poke(0x5e00+row,disp[row])
end

function set_note_by_disp(row,play)
	if disp[row]==0 then
		set_note(0,row,0,0,0,0,play)
	elseif disp[row]==1 then
		set_note(0,row,0xc,0,7,3,play)
	elseif disp[row]==2 then
		set_note(0,row,0x20,6,4,5,play)
	elseif disp[row]==3 then
		set_note(0,row,0x3c,4,1,5,play)
	elseif disp[row]==4 then
		set_note(0,row,0x3a,4,2,5,play)
	elseif disp[row]==5 then
		set_note(0,row,0x20,0,2,3,play)
	elseif disp[row]==6 then
		set_note(0,row,0x18,0,2,3,play)
	elseif disp[row]==7 then
		set_note(0,row,0x10,0,3,3,play)
	elseif disp[row]==8 then
		set_note(0,row,0x23,4,2,6,play)
	end
end

function _update()
	row=stat(20)
	if (btnp(5)) then
		if (playing==1) then 
			music(-1)
			curs=0
		else 
			music(0)
			end
		playing=1-playing
	end
	if (playing==1) then
	else
		moved=true
		if (btnp(0)) then
			curs-=1
		elseif (btnp(1)) then
			curs+=1
		else
			moved=false
		end
	 curs=curs%32
	 changed=true
		if (btnp(2)) then
			offset_disp(curs,1)
		elseif (btnp(3)) then
			offset_disp(curs,-1)
		else
			changed=false
		end
		set_note_by_disp(curs,moved or changed)
	end
end

function draw_machine()
	row=stat(20)
	
	sp=1
	if (playing==1) sp+=1
	if (row%4==0 and playing==1) sp+=1
	print("‹‘”ƒ:edit",0,64,5)
	print("—:",108,64,5)
	spr(sp,120,63)

	for a=0,31 do
		nte,tbr,vol,fx=get_note(0,a)
		c=5
		if (a%4==0) c=6
		--if (playing and row==a) c+=8
		x=a*4
		y=48
		line(x,y+6,x+2,y+6,c)
		if playing==1 then
			if (row==a) then
				line(x,y+5,x+2,y+5,c)
			end
		else
			if (curs==a) then
				line(x,y+5,x+2,y+5,c)
			end
		end
		if disp[a]==1 then
			print("b",x,y,12)
		elseif disp[a]==2 then
			print("s",x,y,11)
		elseif disp[a]==3 then
			print(".",x,y,14)
		elseif disp[a]==4 then
			print(":",x,y,15)
		elseif disp[a]==5 then
			print("@",x,y,10)
		elseif disp[a]==6 then
			print("@",x,y,9)
		elseif disp[a]==7 then
			print("@",x,y,8)
		elseif disp[a]==8 then
			print("%",x,y,7)
		end
	end
end

function _draw()
	cls(0)
	draw_machine()
end

