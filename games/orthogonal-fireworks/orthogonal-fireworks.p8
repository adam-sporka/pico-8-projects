pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
-- orthogonal fireworks
-- by @adam_sporka

max_speed=15
viscosity=0.1
weakening=0.6
exploded=0

objects={}
function new_object(x,y,vx,vy,e)
 new={}
 new.state=0
 new.phase=0
 new.x,new.y=x,y
 new.vx,new.vy=vx,vy
 new.color=flr(rnd(3))*16+16
 new.energy=e*(0.25+rnd(weakening))
 new.speed=max_speed*e*rnd(weakening)+0.5
 add(objects,new)
end

function draw_object(obj)
 if obj.state==0 then
  spr(1,obj.x-3,obj.y-3)
 else
  spr(obj.phase+obj.color,obj.x-3,obj.y-3)
 end
end

ch=0
activity=false

function animate_object(obj)
 if obj.state==0 then
  obj.speed=(1-viscosity)*obj.speed
  obj.x+=obj.speed*obj.vx
  obj.y+=obj.speed*obj.vy
  activity=true
  if obj.speed<0.1 then
   obj.state=1
  end
 elseif obj.state==1 then
  if obj.phase==3 then
   sfx(ch,ch)
   ch+=1
   ch=ch%4
   exploded+=1
  end
  if obj.phase==3 and obj.energy>0.1 then
   if rnd(1)<0.9 then
    dirx={-1,0,1,0}
    diry={0,-1,0,1}
   else
    dirx={-1,1,1,-1}
    diry={-1,-1,1,1}
   end
   for a=1,4 do
    new_object(obj.x,obj.y,dirx[a],diry[a],obj.energy)
   end
  end
  if obj.phase<15 then
   obj.phase+=1
   activity=true
  end
 end
end

function _init()
 new_object(64,64,0,0,1)
 objects[1].state=1
end

function reset()
 objects={}
 _init()
 exploded=0
end

function _update()
 if btnp(4) then
  reset()
 end
 activity=false
 for a=1,#objects do
  active_now=animate_object(objects[a])
  if active_now then activity=true end
 end
end

function _draw()
 rectfill(0,0,128,128,0)
 for a=1,#objects do
  draw_object(objects[a])
 end
 if not activity then
  print("press 'z' to go again",44,120,6)
 end
 -- print(exploded.."/"..#objects,0,0,7)
 rectfill(0,120,16,128,0)
 print(exploded,0,120,6)
end

