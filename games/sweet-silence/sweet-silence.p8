pico-8 cartridge // http://www.pico-8.com
version 7
__lua__
problem={}
sel_fader=1
keys={}
xx=38
yy=40

function is_held(k) return band(keys[k], 1) == 1 end
function is_pressed(k) return band(keys[k], 2) == 2 end
function is_released(k) return band(keys[k], 4) == 4 end

function upd_key(k)
	if keys[k] == 0 then
		if btn(k) then keys[k] = 3 end
	elseif keys[k] == 1 then
		if btn(k) == false then keys[k] = 4 end
	elseif keys[k] == 3 then
		if btn(k) then keys[k] = 1
		else keys[k] = 4 end
	elseif keys[k] == 4 then
		if btn(k) then keys[k] = 3
		else keys[k] = 0 end
	end
end

function init_keys()
	for a = 0,5 do keys[a] = 0 end
end

function upd_keys()
	for a = 0,5 do upd_key(a) end
end

function new_problem()
	problem.solution={}
	for a=1,6 do
		problem.solution[a]=flr(rnd(8))
	end
	problem.order={}
	
	count=flr(rnd(6)+1)
	for c=1,count do
		pass=false
		while not pass do
			i=flr(rnd(6)+1)
			pass=true
			for d=1,#problem.order do
				if problem.order[d]==i then
					pass=false
				end
			end
			if pass then
				add(problem.order,i)
			end
		end
	end
	add(problem.order,0)
	problem.settings={0,0,0,0,0,0}
end

function _init()
	init_keys()
	problem.order={0}
	problem.solution={0,0,0,0,0,0}
	problem.settings={0,0,0,0,0,0}
end

function get_dist(idx)
	d=abs(
		problem.settings[idx]-
		problem.solution[idx])
	if (d>2) then return 2 end
	return d
end

function get_pressing_issue(from)
	p_idx=from
	while problem.order[p_idx]>0 do
		d=get_dist(problem.order[p_idx])
		if d>0 then
			return problem.order[p_idx],p_idx,d
		end
		p_idx+=1
	end
end

function _update()
	upd_keys()
	update=0
	if is_pressed(0) then
	sel_fader-=1
	if sel_fader<1 then sel_fader=1 end
	elseif is_pressed(1) then
		sel_fader+=1
	if sel_fader>6 then sel_fader=6 end
	elseif is_pressed(2) then
		problem.settings[sel_fader]+=1
		update=1
	elseif is_pressed(3) then
		problem.settings[sel_fader]-=1
		update=1
	elseif is_pressed(4) then
		new_problem()
		update=1
	end
	if problem.settings[sel_fader]<0 then
		problem.settings[sel_fader]=0
	elseif problem.settings[sel_fader]>7 then
		problem.settings[sel_fader]=7
	end
	if update==1 then
		proxy=nil
		issue,issue_idx,issue_mag=get_pressing_issue(1)
		if issue!=nil then
			if issue_mag==2 then 
				sfx((issue-1)*4+16,0) 
				sfx(-1,1)
			end
			if issue_mag==1 then 
				sfx((issue-1)*4+17,0) 
				proxy=get_pressing_issue(issue_idx+1)
				if proxy!=nil then
					sfx((proxy-1)*4+18,1) 
				else
					sfx(-1,1)
				end
			end
		else
			sfx(-1,0)
			sfx(-1,1)
		end
	end
end

prev_issue=-1
function _draw()
	rectfill(0,0,127,127,0)
	for a=1,#problem.settings do
		spr(2,xx+(a-1)*9,yy+0)
		spr(18,xx+(a-1)*9,yy+17)
		spr(1,xx+(a-1)*9,yy+2)
		spr(1,xx+(a-1)*9,yy+9)
		spr(17,xx+(a-1)*9,yy+(7-problem.settings[a])+2)
	end
	spr(33,xx+(sel_fader-1)*9,yy+20)
	if get_pressing_issue(1)==nil then
	 print("sweet silence",38,70,6)
	 print("press 'z'",46,76,6)
	end
end

