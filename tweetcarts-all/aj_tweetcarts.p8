pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function tri(t)
	c=cos(t)
	if (c < -.5) return -1
	if (c > .5) return 1
	return 0
end

sel=0
::menu::
str={
"spiky sierpinski square",
"two bxor-filled rectangles",
"round goniometric chaos",
"array of circles",
"sum of harmonics (orig.)",
"sum of harmonics (alt.)",
"trigonometric elephant",
"sinusoidal jellyfish",
"glitchy paint",
"micro-fauna in pico-8",
}
t=0
::_::

cls()
if (btnp(2)) sel=(sel-1)%#str
if (btnp(3)) sel=(sel+1)%#str
for a=0,140,8 do
	y=a+cos(t/100)*4
	line(0,y,127,y,5)
	x=a+sin(t/100)*4
	line(x,0,x,127,5)
end
rectfill(0,0,127,19,0)
line(0,19,127,19,6)
rectfill(0,108,127,127,0)
line(0,108,127,108,6)
for a=0,#str-1 do
	c1=6 c2=0
	x=64-#str[a+1]*2
	y=64-(#str*4)+a*8
	if sel==a then
		c1=7 c2=1
		rectfill(0,y,127,y+8,c2)
		for b=1,#str[a+1] do
			c=sub(str[a+1],b,b)
			print(c,x+((b-1)*4),y+2+c2*tri((t+b+a*2)/15),(t/2+b+a*2)%8+8)
		end
	else
		print(str[a+1],x-1,y+2,0)
		print(str[a+1],x+1,y+2,0)
		print(str[a+1],x,y+1,0)
		print(str[a+1],x,y+3,0)
		print(str[a+1],x,y+2,6)
	end
end
print("adamj's tweetcarts 1.0",20,0,5)
print("as tweeted by @adam_sporka",12,8,5)
print("warning: some of these flash!",6,114,5)
print("\x94\x83\x8e select    \x97 returns here",0,122,5)
